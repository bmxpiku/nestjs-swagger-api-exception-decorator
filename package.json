{
  "name": "@nanogiants/nestjs-swagger-api-exception-decorator",
  "version": "1.0.2",
  "description": "NestJS Swagger decorator for API exceptions",
  "main": "./dist/index",
  "types": "./dist/index",
  "scripts": {
    "prepublishOnly": "rimraf dist && tsc",
    "lint": "tslint -p tsconfig.json -c tslint.json"
  },
  "keywords": [
    "nestjs",
    "swagger",
    "api",
    "exception"
  ],
  "license": "MIT",
  "author": "NanoGiants GmbH",
  "repository": {
    "type": "git",
    "url": "https://github.com/nanogiants/nestjs-swagger-api-exception-decorator.git"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.0.7",
    "@nestjs/swagger": "^4.5.1"
  },
  "devDependencies": {
    "@nestjs/common": "^7.3.2",
    "@nestjs/swagger": "^4.5.12",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.7"
  },
  "prettier": {
    "useTabs": false,
    "singleQuote": true,
    "tabWidth": 2,
    "printWidth": 120,
    "trailingComma": "all",
    "arrowParens": "avoid"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  }
}
